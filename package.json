{
  "name": "nuxt3-vuetify-vitest-template",
  "version": "0.1.0",
  "description": "Nuxt 3 + Vuetify + Vitest + ESLint template for component testing",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --no-fork",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.2",
    "@nuxt/test-utils": "^3.15.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.16.0",
    "happy-dom": "^15.11.7",
    "nuxt": "^3.14.1592",
    "sass": "^1.82.0",
    "vitest": "^2.1.8",
    "vuetify-nuxt-module": "^0.18.3"
  }
}
